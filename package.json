{
  "name": "react-aim-menu",
  "version": "1.2.0",
  "description": "A React version of Amazon's jQuery-menu-aim plugin",
  "main": "lib/index.js",
  "module": "lib/module.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "parcel example/index.html",
    "build": "rm -rf lib && tsc",
    "build:parcel": "rm -rf lib && parcel build src/index.tsx --dist-dir lib",
    "test": "npm run lint",
    "lint": "node_modules/.bin/eslint './src/**/*.tsx' './src/**/*.ts'",
    "prepublishOnly": "npm run build:parcel"
  },
  "files": [
    "lib/"
  ],
  "keywords": [
    "react",
    "amazon",
    "jquery-menu-aim",
    "menu-aim"
  ],
  "author": "shaggyrec <i@shagg.ru>",
  "repository": {
    "type": "git",
    "url": "https://github.com/shaggyrec/react-aim-menu.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.1",
    "@parcel/transformer-typescript-types": "^2.0.1",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "parcel": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0"
  }
}
